const nestedArrays = [[1, 2, 3], [0, 0, 0,], [-5, -6, -12], [0], [36, 6, 535]];

const addedInnerArray = [];

function addArrays (x) {
    for (let i = 0; i < x.length; i++) {
        let sum = 0;
        for (let j = 0; j < x[i].length; j++) {
            sum = sum + x[i][j];
            console.log(x[i][j], sum)
        }
        addedInnerArray.push(sum + " line 57")
    }
}

console.log(addArrays(nestedArrays))

